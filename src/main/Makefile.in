# Makefile for somthin'

SHELL=/bin/sh

CC = @CC@ @CFLAGS@
FPP = @FPP@
FPPFLAGS = @FPPFLAGS@
FC = @FC@
LD = @LD@
CFLAGS =  @CFLAGS@ 
FFLAGS = @F2C_H@ @FFLAGS@
RANLIB = @RANLIB@
LDFLAGS = @LDFLAGS@
AR = @AR@

objects = opium.o nlm.o parameter.o

all_objects = $(objects)

this_dir = main

all: $(all_objects)
	-rm -f objects
	for i in $(all_objects); do echo $(this_dir)/$$i >> objects; done

clean:
	rm -f $(objects) libmain.a core* *~ *.f.f objects

distclean: clean
	rm -f Makefile

